  <nav id="sidebar">
    <div class="p-4 pt-5">
      <h1>
        <a href="/" class="logo"><%= PAGE_TITLE %></a>
      </h1>
      <ul class="list-unstyled mb-5">
        <li <%= (typeof menuActive != 'undefined' ? (menuActive == 'home' ? 'class=active' : '') : '') %>>
          <a href="/">Inicio</a>
        </li>
        <% if(galleryAvailable.length> 0){ %>
          <% for (gallery of galleryAvailable) { %>
            <li <%= (typeof menuActive != 'undefined' ? (menuActive == gallery ? 'class=active' : '') : '') %>>
              <a href="<%= gallery.toLowerCase() %>">
                <%= gallery %>
              </a>
            </li>
          <% } %>
        <% } %>
      </ul>
      <div class="footer">
        <p><%= new Date().getFullYear();%> &copy; <%= PAGE_TITLE %>.</p>
      </div>
    </div>
  </nav>